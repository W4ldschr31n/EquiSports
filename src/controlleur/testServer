from bottle import *

@route('/hello')
def hello():
    return "Hello World!"

@route('/print/<data>')
def afficher(data = "Bonjour"):
    return template("La page vous dit : {{data}}", data=data)

@route('/')
def accueil():
    return static_file("index", root='./')















run(host='localhost', port=8665, debug=True)
